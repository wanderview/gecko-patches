+NS_IMETHODIMP
+InterceptedHttpChannel::CancelInterception(nsresult aStatus)
+{
+  nsAutoCString spec;
+  if (mURI) {
+    mURI->GetSpec(spec);
+  }
+  nsAutoCString name;
+  GetErrorName(aStatus, name);
+  printf_stderr("### ### [%p] InterceptedHttpChannel::%s - %s, %s\n", this, __func__, name.get(), spec.get());
+
+  if (mPump) {
+    return mPump->Cancel(aStatus);
+  }
+
+  return AsyncAbort(aStatus);
+}
