+class Cache::FetchHandler final : public PromiseNativeHandler
+{
+public:
+  FetchHandler(Cache* aCache, Promise* aPromise)
+    : mCache(aCache)
+    , mPromise(aPromise)
+  {
+    MOZ_ASSERT(mCache);
+    MOZ_ASSERT(mPromise);
+  }
+
+  virtual void
+  ResolvedCallback(JSContext* aCx, JS::Handle<JS::Value> aValue) override
+  {
+    nsTArray<nsRefPtr<Response>> responseList;
+
+    JS::Rooted<JSObject*> obj(aCx, &aValue.toObject());
+
+    if (JS_IsArrayObject(aCx, obj)) {
+      uint32_t length;
+      if (NS_WARN_IF(!JS_GetArrayLength(aCx, obj, &length))) {
+        Fail();
+        return;
+      }
+
+      for (uint32_t i = 0; i < length; ++i) {
+        JS::RootedValue value(aCx);
+
+        if (NS_WARN_IF(!JS_GetElement(aCx, obj, i, &value))) {
+          Fail();
+          return;
+        }
+
+        JS::Rooted<JSObject*> responseObj(aCx, &value.toObject());
+
+        Response* r = nullptr;
+        nsresult rv = UNWRAP_OBJECT(Response, responseObj, r);
+        if (NS_WARN_IF(NS_FAILED(rv))) {
+          Fail();
+          return;
+        }
+
+        nsRefPtr<Response> response = r;
+        if (!response->Ok()) {
+          Fail();
+          return;
+        }
+
+        responseList.AppendElement(Move(response));
+      }
+    } else {
+      Response* r = nullptr;
+      nsresult rv = UNWRAP_OBJECT(Response, obj, r);
+      if (NS_WARN_IF(NS_FAILED(rv))) {
+        Fail();
+        return;
+      }
+
+      nsRefPtr<Response> response = r;
+      if (!response->Ok()) {
+        Fail();
+        return;
+      }
+
+      responseList.AppendElement(Move(response));
+    }
+
+    // TODO: call Cache::PutAll() or something
+  }
+
+  virtual void
+  RejectedCallback(JSContext* aCx, JS::Handle<JS::Value> aValue) override
+  {
+    NS_ASSERT_OWNINGTHREAD(FetchHandler);
+    Fail();
+  }
+
+private:
+  ~FetchHandler()
+  {
+  }
+
+  void
+  Fail()
+  {
+    ErrorResult rv;
+    rv.ThrowTypeError(MSG_FETCH_FAILED);
+    mPromise->MaybeReject(rv);
+  }
+
+  nsRefPtr<Cache> mCache;
+  nsRefPtr<Promise> mPromise;
+
+  NS_DECL_ISUPPORTS
+};
