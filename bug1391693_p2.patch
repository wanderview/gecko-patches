+  OnDataAvailable(nsIRequest* aRequest, nsISupports* aContext,
+                  nsIInputStream* aInputStream, uint64_t aOffset,
+                  uint32_t aCount) override
+  {
+    return mListener->OnDataAvailable(mChannel, aContext, aInputStream,
+                                      aOffset, aCount);
+  }
+};
